@model IEnumerable<Inqury.Models.Entry>

<h5 class="page_title">トップ画面</h5><br>

@if (ViewBag.cnt == 0)
{
    <p>未登録のお問合せはありません。</p>
}
else
{
    <p>未登録のお問合せがあります。</p>
}
<table class="table table-bordered table-sm table-font" style="text-align:center;">
    <tr class="heading">
        <th>会社名</th>
        <th>担当者名</th>
        <th>電話番号</th>
        <th>着信時刻</th>
        <th>終了時刻</th>
        <th>登録</th>
    </tr>

    @foreach (var data in Model)
    {
        <tr class="table_back">
            <td>@data.Company_Name</td>
            <td>@data.Tan_Name</td>
            <td>@data.Tel_No</td>
            <td>@data.Entry_Time</td>
            <td>@data.End_Time</td>
            <td><a asp-route="Entry" asp-route-id="@data.Id" class="select_btn">SELECT</a></td>
        </tr>
    }
</table>