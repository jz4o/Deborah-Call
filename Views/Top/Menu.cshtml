@model IEnumerable<Inqury.Models.Entry>

<h5 class="page_title">着信情報</h5><br>

@if (ViewBag.cnt == 0)
{
    <p>未登録のお問合せはありません。</p>
}
else
{
    <p style="color:#dc143c;font-weight:bold;">未登録のお問合せがあります。</p>
}
<partial name="../Shared/_errors.cshtml" />
<table class="table table-bordered table-sm table-font" style="text-align:center;">
    <tr class="heading">
        <th style="width:250px;">問 合 せ 元</th>
        <th>担 当 者 名</th>
        <th style="width:150px;">電 話 番 号</th>
        <th style="width:170px;">着 信 時 刻</th>
        <th style="width:170px;">終 了 時 刻</th>
        <th style="width:130px;">操 作</th>
    </tr>

    @await Html.PartialAsync("_Menu_List.cshtml")

</table>